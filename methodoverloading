class method
 {
  int sum(int x,int y)
    {
     return(x+y);
     }
  int sum(int x,int y,int a)
    {
      return(x+y+a);
    }
}
class sum
{
  public static void main(String args[])
    {
     method s=new method();
     System.out.println(s.sum(1,2));
     System.out.println(s.sum(1,2,3));
}
}
