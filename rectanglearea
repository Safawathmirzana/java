import java.util.Scanner;
class Rect
{
     public static void main (String args[])
     {
     System.out.println("enter the length and breadth");
     Scanner input=new Scanner(System.in);
     int l,b,area;
     l=input.nextInt();
     b=input.nextInt();
     area=l*b;
     System.out.println("area of rectangle is"+area);
}
}
